package com.ilpexperion.GroupAssignment;

import java.util.Scanner;

public class Descending3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scanner = new Scanner(System.in);
		int number[];
		number=new int[20];
		int temp;
		for(int intiger=1;intiger<4;intiger++)
			{
			System.out.println("Enter Number"+intiger+"= ");
			number[intiger]=scanner.nextInt();
			}
		for(int intiger=1;intiger<4;intiger++)
		{
			if(number[intiger]<number[intiger+1])
			{
				temp=number[intiger];
				number[intiger]=number[intiger+1];
				number[intiger+1]=temp;
			}
			else
			{
				continue;
			}
		}
		for(int intiger=1;intiger<4;intiger++)
		{
		System.out.println("Enter Number"+intiger+"= "+number[intiger]);
		}
		
	}

}
