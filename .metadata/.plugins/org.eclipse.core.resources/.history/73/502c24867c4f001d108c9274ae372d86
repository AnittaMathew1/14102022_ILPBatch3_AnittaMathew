package com.experion.entity;

import java.util.ArrayList;

public class Card extends Product {
	private String cardType;
	ArrayList<Product>bankingServices=new ArrayList<Product>();
	
	
	
	public Card(String productCode, String productName, String productType, String cardType,
			ArrayList<Product> bankingServices) {
		super(productCode, productName, productType);
		this.cardType = cardType;
		this.bankingServices = bankingServices;
	}
	public String getCardType() {
		return cardType;
	}
	public void setCardType(String cardType) {
		this.cardType = cardType;
	}
	public ArrayList<Product> getBankingServices() {
		return bankingServices;
	}
	public void setBankingServices(ArrayList<Product> bankingServices) {
		this.bankingServices = bankingServices;
	}
	
	

}
