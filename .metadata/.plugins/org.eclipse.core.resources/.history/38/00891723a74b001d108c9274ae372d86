package com.ilpexperion.ObjectClassBasics;

public class Employee {
	String name="Anitta";
	int age=22;
	char gender='F';
	double cgpa=7.0;
	
	public void displayEmployDetails() {
		System.out.println("**********Welcome to ILP Program**********");
		System.out.println("Name\t\t\tAge\t\tGender\t\tCGPA");
		System.out.println(name+"			"+age+"		"+gender+"		"+cgpa);
	}
		

}
